{
    "name": "floatingpointsoftware/forum",
    "description": "Laravel 4 forum package by Floating Point Software.",
    "authors": [
        {
            "name": "Kirk Bushell",
            "email": "torm3nt@gmail.com"
        },
	    {
		    "name": "Mike Dugan",
		    "email": "mike@mjdugan.com"
	    }
    ],
    "require": {
        "php": ">=5.5.0",
	    "illuminate/html": "~5.0",
	    "illuminate/support": "5.0.*",
	    "laracasts/commander": "~1.0"
    },
	"require-dev": {
		"mockery/mockery": "dev-master@dev",
		"orchestra/testbench": "3.0.*",
		"phpunit/phpunit": "4.2.*"
	},
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "FloatingPoint\\Grapevine\\": "src/Grapevine",
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "git://github.com/orchestral/phpseclib.git"
		}
	]
}
